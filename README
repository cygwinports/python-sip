  /usr/lib/python2.4/site-packages/sip.dll
  /usr/lib/python2.4/site-packages/sipconfig.py
  /usr/lib/python2.4/site-packages/sipconfig.pyc
  /usr/lib/python2.4/site-packages/sipconfig.pyo
  /usr/lib/python2.4/site-packages/sipdistutils.py
  /usr/lib/python2.4/site-packages/sipdistutils.pyc
  /usr/lib/python2.4/site-packages/sipdistutils.pyo
  /usr/share/doc/Cygwin/python-sip-4.6.README
  /usr/share/doc/python-sip-4.6/ChangeLog
  /usr/share/doc/python-sip-4.6/LICENSE
